# Production Environment Variables for React App on Vercel

# API Configuration - Points to Render backend
REACT_APP_API_BASE_URL=https://thedigitaltrading-api.onrender.com
REACT_APP_SOCKET_URL=wss://thedigitaltrading-api.onrender.com
REACT_APP_FRONTEND_URL=https://thedigitaltrading.vercel.app

# App Configuration
REACT_APP_APP_NAME=The Digital Trading Platform
REACT_APP_VERSION=1.0.0

# Features
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Performance
REACT_APP_ENABLE_SERVICE_WORKER=true