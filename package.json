{
  "name": "thedigitaltrading-platform",
  "version": "1.0.0",
  "description": "The Digital Trading Platform - Full Stack Application",
  "main": "server/server.js",
  "scripts": {
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "test": "cd server && npm test && cd ../client && npm test",
    "deploy-prep": "bash quick-deploy.sh",
    "render-build": "cd server && npm install",
    "render-start": "cd server && npm start",
    "vercel-build": "cd client && npm run build",
    "postinstall": "cd server && npm install"
  },
  "keywords": [
    "trading",
    "platform",
    "react",
    "nodejs",
    "mongodb",
    "redis",
    "render",
    "vercel"
  ],
  "author": "The Digital Trading Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicrey1/thedigitaltrading.git"
  },
  "homepage": "https://www.thedigitaltrading.com",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/lib-storage": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "bitcoinjs-lib": "^5.2.0",
    "ethers": "^5.7.2",
    "form-data": "^4.0.4",
    "lucide-react": "^0.537.0",
    "mongoose": "^8.16.5",
    "react-toastify": "^11.0.5",
    "sharp": "^0.34.3",
    "socket.io-client": "^4.8.1",
    "tronweb": "^2.9.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
